{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "modulePaths": ["<rootDir>"],
  "globalSetup": "<rootDir>/test/jest-e2e.setup.ts",
  "globalTeardown": "<rootDir>/test/jest-e2e.teardown.ts",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
